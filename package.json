{
  "name": "support-docs",
  "version": "0.2.0",
  "private": true,
  "engines": {
    "node": ">=20.15.0"
  },
  "scripts": {
    "predev": "npm run build:images",
    "prebuild": "npm run build:images",
    "prebuild:test": "npm run build:images",
    "dev": "next dev",
    "build": "next build",
    "build:ssr": "next build && next export",
    "build:index:analyst": "node ./scripts/analystIndex.mjs",
    "build:index:momentum": "node ./scripts/momentumIndex.mjs",
    "build:index:support": "node ./scripts/supportIndex.mjs",
    "build:images": "rimraf public/content && node ./scripts/copyImages.mjs",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "precommit": "lint-staged",
    "test": "npm run build:test && npm run test:e2e:headless",
    "build:test": "ENV=test next build",
    "test:ci": "start-server-and-test http://localhost:3000 test:e2e:headless",
    "test:e2e:headless": "ENV=test node_modules/.bin/cypress run",
    "test:e2e:gui": "ENV=test node_modules/.bin/cypress open"
  },
  "lint-staged": {
    "**/*.{tsx,ts,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@segment/snippet": "^4.16.2",
    "@sentry/react": "^6.19.7",
    "@sentry/tracing": "^6.19.7",
    "@sentry/webpack-plugin": "^1.21.0",
    "@sparkpost/design-tokens": "^6.1.0",
    "@sparkpost/matchbox": "^6.1.0",
    "@sparkpost/matchbox-icons": "^6.1.0",
    "@styled-system/css": "^5.1.5",
    "@types/uuid": "^8.3.4",
    "framer-motion": "^5.6.0",
    "glob": "^7.2.3",
    "gray-matter": "^4.0.3",
    "isbot": "^3.8.0",
    "js-cookie": "^3.0.5",
    "next": "^12.3.7",
    "react": "^17.0.2",
    "react-day-picker": "^7.4.10",
    "react-dom": "^17.0.2",
    "react-instantsearch-dom": "^6.40.4",
    "react-markdown": "^7.1.2",
    "react-syntax-highlighter": "^15.6.1",
    "rehype-raw": "^6.1.1",
    "remark": "^14.0.3",
    "remark-code-blocks": "^2.0.1",
    "remark-gfm": "^3.0.1",
    "remark-html": "^15.0.2",
    "styled-components": "^5.3.11",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^4.41.3",
    "@sentry/types": "^6.19.7",
    "@types/glob": "^7.2.0",
    "@types/gtag.js": "^0.0.7",
    "@types/js-cookie": "^3.0.6",
    "@types/react": "^17.0.87",
    "@types/react-dom": "^17.0.26",
    "@types/react-instantsearch-dom": "^6.12.9",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@types/styled-components": "^5.1.34",
    "@types/styled-system": "^5.1.23",
    "@types/styled-system__css": "^5.0.21",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "algoliasearch": "^4.25.2",
    "cypress": "^9.7.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-flowtype": "^6.1.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "next-sitemap": "^1.9.12",
    "prettier": "2.4.1",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.15.4",
    "strip-markdown": "^5.0.1",
    "typescript": "4.4.4",
    "yaml-loader": "^0.6.0"
  }
}
